<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Nopcommerce-Sanity Test" verbose="1" >
    <groups>
        <run>
            <include name="sanity"></include>
        </run>
    </groups>
    <listeners>
        <listener class-name="uk.co.expedia.listeners.Listeners"></listener>
    </listeners>
    <test name="Sanity Test" >
        <classes>
            <class name="uk.co.expedia.testsuit.LoginTest"/>
            <class name="uk.co.expedia.testsuit.FlightTest"/>


        </classes>
    </test>
</suite>