<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Expedia Test" verbose="1" >
    <listeners>
        <listener class-name="uk.co.expedia.listeners.Listeners"></listener>
        <listener class-name="org.uncommons.reportng.HTMLReporter"></listener>
        <listener class-name="org.uncommons.reportng.JUnitXMLReporter"></listener>
    </listeners>
    <test name="Login Test" >
        <classes>
            <class name="uk.co.expedia.testsuit.LoginTest" />
        </classes>
    </test>
    <test name="Flight Test" >
        <classes>
            <class name="uk.co.expedia.testsuit.FlightTest" />
        </classes>
    </test>

</suite>
